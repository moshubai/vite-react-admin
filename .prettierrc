{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120, 
  "useTabs": false, 
  "jsxSingleQuote": false, 
  "trailingComma": "all",
  "bracketSpacing": true, 
  "jsxBracketSameLine": false, 
  "arrowParens": "always", 
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
